{
  "name": "io.maana.template",
  "author": "Maana, Inc.",
  "license": "UNLICENSED",
  "private": true,
  "version": "2.0.0",
  "description": "Maana Q Knowledge Service template",
  "main": "src/start.js",
  "repository": "git@github.com:maana-io/Q-ksvc-templates.git",
  "scripts": {
    "precommit": "lint-staged",
    "prettier": "./node_modules/.bin/prettier --write \"src/**/*.{js,jsx}\"",
    "start": "node ./dist/start.js",
    "startdev": "node ./dist/start.dev.js",
    "prepublish": "./node_modules/.bin/babel ./src -d ./dist && cp src/*.gql dist",
    "dbg": "nodemon --inspect ./src/start.dev.js --exec babel-node -e js",
    "dev": "nodemon ./src/start.dev.js --exec babel-node -e js",
    "disableAuth": "nodemon ./src/start.dev.js --exec babel-node -e js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": "9.3"
  },
  "dependencies": {
    "@types/amqplib": "^0.5.2",
    "amqplib": "^0.5.2",
    "apollo-cache-inmemory": "^1.1.5",
    "apollo-client": "^2.2.0",
    "apollo-fetch": "^0.7.0",
    "apollo-link-context": "^1.0.3",
    "apollo-link-http": "^1.3.2",
    "apollo-server-express": "^1.3.2",
    "auth0": "^2.9.1",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.0",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.1",
    "express-jwt": "^5.3.0",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "graphql-tools": "^2.16.0",
    "io.maana.shared": "^2.0.0-0",
    "jwks-rsa": "^1.2.1",
    "maana-amqp-pubsub": "^0.1.3-0",
    "nano-fs": "^1.2.4",
    "node-fetch": "^1.7.3",
    "request": "^2.83.0",
    "subscriptions-transport-ws": "^0.9.4",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "lint-staged": "^6.0.0",
    "nodemon": "^1.14.8",
    "prettier": "^1.9.2"
  }
}
